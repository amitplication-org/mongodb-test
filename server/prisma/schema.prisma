datasource mongodb {
  provider = "mongodb"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id       String @id @default(auto()) @map("_id") @mongodb.ObjectId
  /// Field referred in an index, but found no data to define the type.
  username Json?  @unique
}

model Order {
  id String @id @default(auto()) @map("_id") @mongodb.ObjectId
}

model Customer {
  id String @id @default(auto()) @map("_id") @mongodb.ObjectId
}

model Address {
  id String @id @default(auto()) @map("_id") @mongodb.ObjectId
}

model Product {
  id String @id @default(auto()) @map("_id") @mongodb.ObjectId
}